{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "useDefineForClassFields": true,
    "lib": ["ESNext", "DOM"],
    "jsx": "preserve",
    "strict": true,
    "noEmit": true
  },
  "include": ["vite-env.d.ts", "src/**/*.vue", "src/**/*.tsx", "src/**/*.ts"]
}
